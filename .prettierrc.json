{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "printWidth": 140,
  "proseWrap": "preserve",
  "requirePragma": false,
  "useTabs": false,
  "vueIndentScriptAndStyle": true,
  "prettier.quoteProps": "as-needed",
  "prettier.useEditorConfig": true,
  "prettier.packageManager": "yarn"
}
